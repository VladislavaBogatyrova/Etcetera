{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": "src",
    "paths": {
      "@constants/*": ["constants/*"],
      "@screens/*": ["screens/*"],
      "@components/*": ["components/*"],
      "@services/*": ["services/*"],
      "@assets/*": ["assets/*"],
      "@context/*": ["context/*"],

    
    }
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js", "jest.config.js"]
}
